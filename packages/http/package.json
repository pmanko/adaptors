{
  "name": "@openfn/language-http",
  "version": "4.0.0",
  "description": "An HTTP request language package for use with Open Function",
  "homepage": "https://docs.openfn.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/language-http.git"
  },
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build:babel": "node_modules/.bin/babel src -d lib && npm run ast",
    "build:src": "tsup --config ../../tsup.config.js src/index.js",
    "test": "mocha --require @babel/register",
    "test:watch": "mocha -w --require @babel/register",
    "ast": "simple-ast --adaptor ./src/Adaptor.js --output ast.json",
    "postversion": "git push && git push --tags",
    "version": "npm run build && git add -A lib ast.json"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "dist/",
    "ast.json"
  ],
  "dependencies": {
    "@openfn/language-common": "1.7.1",
    "cheerio": "^1.0.0-rc.10",
    "cheerio-tableparser": "^1.0.1",
    "csv-parse": "^4.10.1",
    "esbuild": "^0.15.10",
    "fast-safe-stringify": "^2.0.7",
    "form-data": "^3.0.0",
    "import": "0.0.6",
    "lodash": "^4.17.19",
    "request": "^2.88.2",
    "tough-cookie": "^4.0.0",
    "tsup": "^6.2.3"
  },
  "devDependencies": {
    "@openfn/simple-ast": "^0.4.1",
    "chai": "^4.2.0",
    "deep-eql": "^4.0.0",
    "jsdoc": "^3.6.10",
    "mocha": "^9.2.2",
    "nock": "^13.0.5",
    "sinon": "^9.2.3"
  }
}
