{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "username": {
            "title": "username",
            "type": "string",
            "description": "Magpi username",
            "minLength": 1,
            "examples": [
                "test@openfn.org"
            ]
        },
        "accessToken": {
            "title": "Access Token",
            "type": "string",
            "description": "Magpi access token",
            "writeOnly": true,
            "minLength": 1,
            "examples": [
                "somEThINGkeyish"
            ]
        }
    },
    "type": "object",
    "additionalProperties": true,
    "required": [
        "username",
        "accessToken"
    ]
}
