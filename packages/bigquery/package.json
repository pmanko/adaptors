{
  "name": "language-bigquery",
  "version": "1.0.1",
  "description": "A Google BigQuery language package for use with Open Function",
  "main": "dist/index.cjs",
  "scripts": {
    "build": "pnpm clean && build-adaptor bigquery",
    "test": "mocha --experimental-specifier-resolution=node --no-warnings",
    "test:watch": "mocha -w --experimental-specifier-resolution=node --no-warnings",
    "clean": "rimraf dist types docs",
    "pack": "pnpm pack --pack-destination ../../dist"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "dist/",
    "types/",
    "ast.json",
    "configuration-schema.json"
  ],
  "dependencies": {
    "@google-cloud/bigquery": "^5.2.0",
    "csv-parse": "^4.10.1",
    "import": "0.0.6",
    "json2csv": "^5.0.1",
    "@openfn/language-common": "v1.4.1",
    "language-http": "github:openfn/language-http#v2.4.12",
    "request": "^2.72.0",
    "unzipper": "^0.10.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "assertion-error": "^1.0.1",
    "chai": "^3.4.0",
    "deep-eql": "^0.1.3",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "regenerator-runtime": "^0.13.7",
    "sinon": "^1.17.2",
    "esno": "^0.16.3",
    "@openfn/buildtools": "workspace:^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/adaptors.git"
  },
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "types/index.d.ts"
}