  "license": "LGPLv3",
  "files": [
    "dist/",
    "types/",
    "ast.json",
    "configuration-schema.json"
  ],
  "dependencies": {
    "@openfn/language-common": "^1.6.2"
  },
  "devDependencies": {
    "@openfn/simple-ast": "^0.3.2",
    "assertion-error": "^1.0.1",
    "chai": "^4.2.0",
    "deep-eql": "^4.0.0",
    "mocha": "^7.1.0",
    "nock": "^13.0.5",
    "sinon": "^9.2.3",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "@openfn/buildtools": "workspace:^1.0.0"
  },
  "type": "module",
  "types": "types/index.d.ts"
