{
  "name": "@openfn/language-asana",
  "version": "2.0.1",
  "description": "An adaptor to access objects in Asana",
  "homepage": "https://docs.openfn.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/adaptors.git"
  },
  "main": "dist/index.cjs",
  "scripts": {
    "build": "pnpm clean && build-adaptor asana",
