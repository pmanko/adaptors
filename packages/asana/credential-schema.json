{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "apiVersion": {
            "title": "API Version",
            "type": "string",
            "description": "The Asana API version to use"
        },
        "token": {
            "title": "Token",
            "type": "string",
            "description": "The Asana API token to use"
        }
    },
    "type": "object",
    "additionalProperties": true,
    "required": [
        "apiVersion",
        "token"
    ]
}