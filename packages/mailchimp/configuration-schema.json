{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "server": {
            "title": "Data Center",
            "type": "string",
            "description": "Mailchimp data center for your account",
            "format": "uri",
            "minLength": 1,
            "examples": [
                "us11"
            ]
        },
        "apiKey": {
            "title": "API Key",
            "type": "string",
            "description": "Mailchimp API Key",
            "writeOnly": true,
            "minLength": 1,
            "examples": [
                "somEThINGkeyish"
            ]
        }
    },
    "type": "object",
    "additionalProperties": true,
    "required": [
        "server",
        "apiKey"
    ]
}