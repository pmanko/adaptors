  "dependencies": {
    "@openfn/buildtools": "workspace:^1.0.1",
    "@openfn/language-common": "1.6.2",
    "axios": "^0.24.0",
    "lodash": "^4.17.19",
    "qs": "^6.10.3"
  },
  "devDependencies": {
    "@openfn/simple-ast": "^0.4.0",
    "assertion-error": "^1.0.1",
    "chai": "^3.4.0",
    "chai-http": "^4.3.0",
    "deep-eql": "^0.1.3",
    "esno": "^0.16.3",
    "mocha": "^7.1.1",
    "nock": "^13.0.5",
