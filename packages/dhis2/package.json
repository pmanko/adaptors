{
  "name": "@openfn/language-dhis2",
  "version": "3.1.0",
  "description": "DHIS2 Language Pack for OpenFn",
  "homepage": "https://docs.openfn.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/adaptors.git"
  },
  "main": "dist/index.cjs",
  "scripts": {
    "build": "pnpm clean && build-adaptor dhis2",
