{
  "name": "language-telerivet",
  "version": "0.0.2",
  "description": "telerivet Language Pack for OpenFn",
  "main": "dist/index.cjs",
  "scripts": {
    "build": "pnpm clean && build-adaptor telerivet",
    "test": "mocha --experimental-specifier-resolution=node --no-warnings",
    "test:watch": "mocha -w --experimental-specifier-resolution=node --no-warnings",
    "clean": "rimraf dist types docs",
    "pack": "pnpm pack --pack-destination ../../dist"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "dist/",
    "types/",
    "ast.json",
    "configuration-schema.json"
  ],
  "dependencies": {
    "@openfn/language-common": "workspace:1.7.4",
    "JSONPath": "^0.10.0",
    "lodash-fp": "^0.10.4",
    "superagent": "^1.8.5"
  },
  "devDependencies": {
    "@openfn/buildtools": "workspace:^1.0.2",
    "@openfn/simple-ast": "0.4.1",
    "assertion-error": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "deep-eql": "^0.1.3",
    "esno": "^0.16.3",
    "mocha": "^10.1.0",
    "rimraf": "^3.0.2",
    "superagent-mock": "^1.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/adaptors.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "types": "types/index.d.ts"
}
