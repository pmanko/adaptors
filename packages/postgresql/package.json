{
  "name": "@openfn/language-postgresql",
  "version": "3.4.0",
  "description": "A PostgreSQL Language Pack for OpenFn",
  "homepage": "https://docs.openfn.org",
  "main": "dist/index.cjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/adaptors.git"
  },
  "scripts": {
    "build": "pnpm clean && build-adaptor postgresql",
    "test": "mocha --experimental-specifier-resolution=node --no-warnings",
    "test:watch": "mocha -w --experimental-specifier-resolution=node --no-warnings",
    "clean": "rm -rf dist types docs",
    "pack": "pnpm pack --pack-destination ../../dist"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "dist/",
    "types/",
    "ast.json",
    "configuration-schema.json"
  ],
  "dependencies": {
    "@openfn/language-common": "1.6.2",
    "pg": "^8.3.2",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@openfn/simple-ast": "0.4.1",
    "assertion-error": "2.0.0",
    "chai": "3.5.0",
    "deep-eql": "4.1.1",
    "mocha": "7.2.0",
    "nock": "12.0.3",
    "sinon": "1.17.7",
    "esno": "0.16.3",
    "rimraf": "3.0.2",
    "@openfn/buildtools": "workspace:^1.0.0"
  },
  "type": "module",
  "types": "types/index.d.ts"
}