{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "accountSid": {
            "title": "Account ID",
            "type": "string",
            "description": "Your Twilio account id",
            "writeOnly": true,
            "minLength": 1
        },
        "authToken": {
            "title": "Auth Token",
            "type": "string",
            "description": "Your Twilio auth token",
            "writeOnly": true,
            "minLength": 1
        }
    },
    "type": "object",
    "additionalProperties": true,
    "required": [
        "accountSid",
        "authToken"
    ]
}