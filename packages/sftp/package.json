{
  "name": "@openfn/language-sftp",
  "label": "SFTP",
  "version": "2.0.17-custom",
  "description": "An SFTP language package for use with Open Function (Enhanced with URI fixing, manual connection control, and comprehensive logging)",
  "homepage": "https://docs.openfn.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/adaptors.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./types/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "scripts": {
    "build": "pnpm clean && build-adaptor sftp",
    "build:debug": "pnpm clean && build-adaptor sftp --sourcemap",
    "test": "NODE_OPTIONS='--loader=testdouble --experimental-specifier-resolution=node --no-warnings' mocha test/*.js",
    "test:watch": "NODE_OPTIONS='--loader=testdouble --experimental-specifier-resolution=node --no-warnings' mocha -w test/*.js",
    "test:file": "NODE_OPTIONS='--loader=testdouble --experimental-specifier-resolution=node --no-warnings' mocha test/${FILE}",
    "test:grep": "NODE_OPTIONS='--loader=testdouble --experimental-specifier-resolution=node --no-warnings' mocha test/*.js --grep \"${GREP}\"",
    "test:single": "NODE_OPTIONS='--loader=testdouble --experimental-specifier-resolution=node --no-warnings' mocha",
    "clean": "rimraf dist types docs",
    "pack": "pnpm pack --pack-destination ../../dist",
    "lint": "eslint src"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "dist/",
    "types/",
    "ast.json",
    "configuration-schema.json"
  ],
  "dependencies": {
    "@openfn/language-common": "workspace:*",
    "lodash": "^4.17.21",
    "ssh2-sftp-client": "9.0.4",
    "xlstream": "^2.5.5"
  },
  "devDependencies": {
    "assertion-error": "2.0.0",
    "chai": "^4.3.7",
    "deep-eql": "4.1.1",
    "eslint": "8.26.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "mocha": "^10.2.0",
    "nock": "12.0.3",
    "rimraf": "^3.0.2",
    "testdouble": "^3.16.9"
  },
  "type": "module",
  "types": "types/index.d.ts"
}
