{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "apiUrl": {
            "title": "API URL",
            "type": "string",
            "description": "Beyonic API URL",
            "format": "uri",
            "minLength": 1
        },
        "apiToken": {
            "title": "API Token",
            "type": "string",
            "description": "Beyonic API token",
            "minLength": 1
        }
    },
    "type": "object",
    "additionalProperties": true,
    "required": [
        "apiUrl",
        "apiToken"
    ]
}